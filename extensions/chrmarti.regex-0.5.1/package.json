			"properties": {
				"regex-previewer.enableCodeLens": {
					"scope": "resource",
					"type": "boolean",
					"default": true,
					"description": "Enables code lens for Regex Previewer"
				}
			}
		},
		"commands": [
			{
				"command": "extension.toggleRegexPreview",
				"title": "Toggle Regex Preview In Side-By-Side Editors"
			}
		],
		"keybindings": [
			{
				"command": "extension.toggleRegexPreview",
				"key": "ctrl+alt+m",
				"mac": "cmd+alt+m"
			}
		],
